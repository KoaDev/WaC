          },
          {
            "type": "MyChocolateyPackage/MyChocolateyPackage",
            "name": "Install7Zip",
            "properties": {
              "PackageName": "7zip",
              "Ensure": "Present"
            }
          },
          {
            "type": "MyChocolateyPackage/MyChocolateyPackage",
            "name": "InstallAdobeReader",
            "properties": {
              "PackageName": "adobereader",
              "Ensure": "Present"
            }
          },
          {
            "type": "MyChocolateyPackage/MyChocolateyPackage",
            "name": "InstallBulkRenameUtility",
            "properties": {
