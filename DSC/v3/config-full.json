            "properties": {
              "PackageName": "winmerge",
              "Ensure": "Present"
            }
          }
        ]
      }
    },
    {
      "type": "Microsoft.WinGet/Package",
      "name": "InstallFork",
      "properties": {
        "id": "Fork.Fork",
        "useLatest": true,
        "acceptAgreements": true
