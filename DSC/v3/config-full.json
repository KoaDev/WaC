    {
      "type": "Microsoft.WinGet/Package",
      "name": "InstallChrome",
      "properties": {
        "id": "Google.Chrome",
        "useLatest": true,
        "acceptAgreements": true
      }
    },
    {
      "type": "Microsoft.VisualStudio.DSC/VSComponents",
      "name": "VSComponents",
      "properties": {
        "ProductId": "Microsoft.VisualStudio.Product.Professional",
        "ChannelId": "VisualStudio.17.Release",
        "VSConfigFile": "C:\\WaC\\DSCv3\\resources\\visual-studio\\professional.vsconfig"
      }
    },
    {
      "type": "MyCertificate/MyCertificate",
      "name": "InstallCrtPaca",
      "properties": {
        "Path": "C:\\WaC\\resources\\certificates\\_.cr-paca.fr.crt",
        "Ensure": "Present"
      }
    },
    {
      "type": "MyCertificate/MyCertificate",
      "name": "InstallCACert",
      "properties": {
        "Path": "C:\\WaC\\resources\\certificates\\Autorite de Certification Region SUD Provence-Alpes-Cote d'Azur.crt",
        "Ensure": "Present"
      }
    },
    {
      "type": "MyWindowsDefenderExclusion/MyWindowsDefenderExclusion",
      "name": "ExcludeProjectsFolder",
      "properties": {
        "Type": "Path",
        "Value": "C:\\Projets",
        "Ensure": "Present"
      }
    },
    {
      "type": "MyWindowsDefenderExclusion/MyWindowsDefenderExclusion",
      "name": "ExcludeDotnetToolsFolder",
      "properties": {
        "Type": "Path",
        "Value": "C:\\Users\\%user%\\.dotnet\\tools",
        "Ensure": "Present"
      }
    },
    {
      "type": "MyNodeVersion/MyNodeVersion",
      "name": "UseLTSNodeVersion",
      "properties": {
